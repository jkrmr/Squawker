<%= form_for(@squawk) do |f| %>
  <%= render 'shared/error_messages', object: f.object %>

  <div class="field">
    <%= f.text_field :content,
                    placeholder: "squawk it!" %>
  </div>
  <div id="countdown"></div>


  <%= f.submit "squawk it!", class: "btn btn-primary offscreen", id: "submit-squawk" %>

<% end %>



